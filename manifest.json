{
  "manifest_version": 2,

  "name": "Nomoogle",
  "description": "This extension is a starting point to create a real Chrome extension",
  "version": "0.0.1",

  "browser_action": {
    "default_popup": "index.html",
    "default_title": "Open the popup"
  },
  "permissions": [
    "tabs",
    "background",
    "storage"
  ],
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  },
  "background": {
    "scripts": ["js/jquery.js","js/background.js"]
  },
  "icons": {
    "16": "img/nomoogle.png",
    "48": "img/nomoogle.png",
    "128": "img/nomoogle.png"
  },
  "content_scripts": [{
    "matches": ["http://*/*", "https://*/*"],
    "js": ["js/jquery.js", "js/content.js"],
    "css": ["css/content.css"],
    "run_at": "document_end"
  }]
}